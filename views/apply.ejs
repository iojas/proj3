<div class="container">
  <form>
    <div class="form-group row">
      <label class="col-sm-2 col-form-label">Name</label>
      <div class="col-sm-10">
        <input class="form-control" placeholder="Please enter your First Name">
      </div>
    </div>
    <div class="form-group row">
      <label class="col-sm-2 col-form-label">Last Name</label>
      <div class="col-sm-10">
        <input class="form-control" placeholder="Please Enter Last Name:">
      </div>
    </div>

    <div class="form-group row">
      <label for="example-tel-input" class="col-sm-2 col-form-label">Telephone</label>
      <div class="col-xs-10">
        <input class="form-control" type="tel" PlaceHolder="10 digit phone number" id="example-tel-input">
      </div>
    </div>

    <div class="form-group row">
          <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
          <div class="col-sm-10">
            <input type="email" class="form-control" id="inputEmail3" placeholder="Email">
          </div>
    </div>

    <fieldset class="form-group row">
      <label class="col-sm-2 col-form-label">Catagory</label>
      <div class="col-sm-10">
        <div class="form-check">
          <label class="form-check-label">
            <input class="form-check-input" type="radio" name="gridRadios" id="gridRadios1" value="10th Pass" checked>
            10th
          </label>
        </div>
        <div class="form-check">
          <label class="form-check-label">
            <input class="form-check-input" type="radio" name="gridRadios" id="gridRadios2" value="12th Pass">
            12th
          </label>
        </div>

        <div class="form-check">
           <label class="form-check-label">
              <input class="form-check-input" type="radio" name="gridRadios" id="gridRadios2" value="PHD">
              PHD
           </label>
        </div>

        <div class="form-check">
            <label class="form-check-label">
                <input class="form-check-input" type="radio" name="gridRadios" id="gridRadios2" value="Other">
                Other
            </label>
        </div>


      </div>
    </fieldset>



    <div class="form-group row">
      <div class="offset-sm-2 col-sm-10">
        <button type="submit" class="btn btn-primary">Apply</button>
      </div>
    </div>
  </form>
