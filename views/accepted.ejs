<div class="container-fluid">
    <div class="row">

        <% if (ojas.length) { %>
        <table class="table table-hover">

            <thead>
            <tr>
                <th>Applicant Name</th>
                <th>Applying for</th>
            </tr>
            </thead>

            <% ojas.forEach(function(app){ if(app.status==1) %>


            <tbody>
            <tr>
                <td>
                    <%=app.name%> <%=app.lname%>
                </td>

                <td>
                    <%=app.catagory%> <%=app.comments%>
                </td>
            </tr>


            <% });;; %>

            <% } %>
            </tbody>
        </table>

    </div>

